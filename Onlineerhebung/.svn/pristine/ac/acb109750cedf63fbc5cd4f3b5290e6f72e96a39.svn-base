package at.emikat.gemerh.ooe

class User {
	
	User (String name, String passwort) {
		setName(name)
		setPasswort(passwort)
	}
	
	String name
	String passwort
	
	private String setName(String name) {
		this.name = name
	}
	
	private String setPasswort(String passwort) {
		this.passwort = passwort
	}
}
